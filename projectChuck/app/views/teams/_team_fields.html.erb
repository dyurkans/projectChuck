<div class="fields">
  <div class="min_attraction">
    <%= f.input :bracket_id, :collection => Bracket.all, :include_blank => false, :selected => (@bracket.id unless @bracket.nil?) %>
	<%= f.input :name, :collection => @teams, :include_blank => false, :selected => @team.name %>
    <%= f.input :coach, :placeholder => "Full Name" %>
    <%= f.input :max_students, collection: [10,9,8,7,6,5], :include_blank => false, :selected => @team.max_students %>
  </div>
</div>