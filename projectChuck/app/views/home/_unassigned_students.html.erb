<h4> Unassigned Registered Students </h4>
<% if @unassigned_students.empty? %>
  &nbsp;&nbsp;<em>None at this time</em>
<% else %>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Age(as of June 1)</th>
      </tr>
    </thead>
    <tbody>
      <% @unassigned_students.each do |student| %>
        <tr>
          <td><%= link_to student.name, student_path(student) %></td>
          <td><%= student.age_as_of_june_1 %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <div class="digg_pagination">
    <div clas="page_info">
      <%= page_entries_info @unassigned_students %>
    </div>
    <%= will_paginate @unassigned_students, :param_name => 'unassigned_student_page', :container => true, :previous_label => "Previous", :next_label => "Next", :inner_window => 2 %>
  </div>
<% end %>